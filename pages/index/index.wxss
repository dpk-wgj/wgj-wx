page{
  width: 100%;
  height: 100vh;
  /* background-color: #f4f6f8; */
  background: #fff
}
.section{
  height: 9%; 
  width: 100%;
  background: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  box-shadow: 0px 5px 5px rgb(200, 200, 200);
}
.nav-all{
  height:30rpx;
  width: 30rpx;
  position: absolute;
  z-index: 999;
  right: 0rpx;
  top: 0;
  background-color: #fff;  
  transform: scale(.18);
  margin-right: 80rpx;
}
.nav-all image{
  width: 280rpx; 
}
.nav-all .point{
  width: 60rpx;
  height: 60rpx;
  border: 1px solid red;
  background: red;
  border-radius: 30rpx;
  position: absolute;
  top: 100rpx;
  left: 200rpx;
}
.nav-me{
  height:30rpx;
  width: 30rpx;
  left: 0;
  margin-top: 10rpx;
  background-color: #fff;
  position: absolute;
  transform: scale(.15);
  z-index: 999;
}
map{
  width: 100%;
  height: 60%; 
  position: absolute;
  top: 9%;
}
.content{
  background: yellow;
}
.circle-str{
  position: absolute; 
  height: 10px;
  width: 10px;
  border-radius: 50%;
  background: #3cbca3;
  left: 40rpx; 
  top: 74.5%; 
} 
 .input-str{
  position: absolute;
  top: 75%;
  height: 4%;
  left: 105rpx;
  border-bottom: 1px solid rgb(200,200,200);
  width: 100%;
  line-height: 13%;  
}
.input-end{
  position: absolute;
  height: 8%;
  width: 100%;
  line-height: 13%;
  top: 80%;
  left: 105rpx;
  border-bottom: 1px solid rgb(200,200,200);
}
.circle-end{
  position: absolute;
  height: 10px;
  width: 10px;
  border-radius: 50%;
  background: #fc9153;
  left: 40rpx;
  top: 83%;
}
.btn{
  position: absolute;
  width: 90%;
  height: 100rpx;
  background-color: #4a4c5b;
  margin-left: 5%;
  bottom: 1%;
  color: #fff;
  left: 0;
}